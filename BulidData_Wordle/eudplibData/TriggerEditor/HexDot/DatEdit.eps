
function UnitDat(unitId)
{
	// unit size l, u, r, d
    wwrite(0x65FD00 + 6856 + unitId * 8, 1);
    wwrite(0x65FD00 + 6858 + unitId * 8, 1);
    wwrite(0x65FD00 + 6860 + unitId * 8, 1);
    wwrite(0x65FD00 + 6862 + unitId * 8, 1);
    // set special ability flag
    dwwrite(0x65FD00 + 17280 + unitId * 4, 0x20400804);
    // edit flag
    wwrite(0x65FD00 + 6168 + unitId * 2, 343);
}

